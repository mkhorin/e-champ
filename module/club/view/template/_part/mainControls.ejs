<!-- _part/mainControls -->

<div class="main-controls row">
    <div class="col-lg-3 col-md-6">
        <button class="btn-success btn" data-command="create">
            <span data-t="">Create</span>
        </button>
    </div>
    <div class="col-lg-3 col-md-6">
        <button class="btn-primary btn" data-command="join">
            <span data-t="">Join</span>
        </button>
    </div>
    <div class="col-lg-3 col-md-6">
        <button class="btn-danger btn" data-command="play">
            <span data-t="">Play</span>
        </button>
    </div>
    <div class="col-lg-3 col-md-6">
        <button class="btn-secondary btn" data-command="playback">
            <span data-t="">Playback</span>
        </button>
        <input type="file" class="play-file hidden" accept="application/json">
    </div>
</div>