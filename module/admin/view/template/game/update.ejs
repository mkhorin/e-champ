<%
layout(_view.get('_layout/frame/form'), {
  params: {
    id: game.name,
    save: false
  }
});
block('tab', 'Game');
block('title', game.label);

const staticTemplate = _view.get('_part/attr/static');
%>

<%- include(staticTemplate, {
  attr: 'name',
  value: game.name
}) %>

<%- include(staticTemplate, {
  attr: 'label',
  value: game.label,
  translateValue: game.name
}) %>

<%- include(staticTemplate, {
  attr: 'webPage',
  value: _format(game.webPage, 'url', {target: '_blank'}),
  escape: false
}) %>

<%- include(staticTemplate, {
  attr: 'maxPlayers',
  value: game.maxPlayers
}) %>

<%- include(_view.get('_part/attr/custom'), {
  attr: 'bots',
  content: {
    template: _view.getInternalTemplate('_bots')
  }
}) %>

<%- include(staticTemplate, {
  attr: 'defaultBot',
  value: game.defaultBot.translateLabel(_controller.language)
}) %>

